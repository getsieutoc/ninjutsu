    "@emotion/styled": "11.11.0",
    "@formatjs/intl-localematcher": "0.5.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.6.0",
    "@tinymce/tinymce-react": "4.3.2",
    "@types/negotiator": "0.6.3",
    "@upstash/ratelimit": "0.4.4",
    "@vercel/kv": "1.0.0",
    "argon2": "0.31.2",
    "date-fns": "2.30.0",

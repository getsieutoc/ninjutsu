    "next": "14.0.3",
    "next-auth": "4.24.5",
    "next-client-cookies": "1.1.0",
    "nodemailer": "6.9.5",
    "qs": "6.11.2",
    "nodemailer": "6.9.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fast-compare": "3.2.2",
    "react-icons": "4.12.0",
    "react-use": "17.4.0",
